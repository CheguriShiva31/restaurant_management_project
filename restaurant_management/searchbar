from django.shortcuts import render
from.models import MenuItem
def menu_view(request):
query = request.GET.get('q')
if query:
   menu_items = MneuItem.objects.filter(name__icontain s=query)
   else:
   menu_items = MneuItem.objects.all()
   return render(request,'menu.html',{'menu.items':menu_items,'query':query})