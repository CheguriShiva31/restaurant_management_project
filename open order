from django .db import models
class OrderStatus(models.Model):
name = models.CharField(max_length=50)

  def __str__(self):
  return self.name


  #add the foreign key tp OrderStatus
  status = models.ForeignKey(OrderStatus,on_delete=mode;s.SET_NULL, null=True,blank=True)
   def __str__(self):
   return f"Order #Order #{self.id} - {self.customer_name}"